var initialParams = [0.5];

var prevParams = [Vector(globalStore.params ? _.values(globalStore.params) : initialParams)];

console.log('prevParams', prevParams);

var newParams = Optimize(function(){
  var params = registerParamsByName('globalParams', prevParams);
  var p = T.get(params[0], 0);
  var dist = Bernoulli({ p: p });
  observe(dist, true);
  observe(dist, true);
  observe(dist, true);
}, { estimator: 'ELBO2', steps: 500, stepSize: 0.01 });

newParams
